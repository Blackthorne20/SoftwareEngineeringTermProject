<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mwsu.www.check_in">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/phonelogo"
        android:label="Check-In"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpAs" />
        <activity android:name=".StudentSignUpOne" />
        <activity android:name=".StudentSignUpTwo" />
        <activity android:name=".InstructorSignUpOne" />
        <activity android:name=".StudentHome" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".QRScanner" />
        <activity android:name=".QRScannerCheckin" />
        <activity android:name=".InstructorSignUpTwo"></activity>
    </application>

</manifest>